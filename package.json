  "repository": {
    "type": "git",
    "url": "https://github.com/sebasslash/simple-thumbnail-ts.git"
  },
  "devDependencies": {
    "@types/duplexify": "^3.6.0",
    "@types/node": "^12.0.0",
    "all-contributors-cli": "^5.4.1",
    "ava": "^1.0.1",
    "coveralls": "^3.0.2",
    "ffmpeg-static": "^2.4.0",
    "fs-extra": "^7.0.1",
    "looks-same": "^3.3.0",
    "nock": "^9.6.1",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.13",
    "standard": "^12.0.1",
    "travis-deploy-once": "^5.0.9",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "duplexify": "^4.0.0"
  }
