{
  "name": "simple-thumbnail-ts",
  "version": "1.0.2",
  "description": "Produce thumbnails from images, videos and URLs with Typescript. Forked from simple-thumbnail",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Philip Scott <pscott@zeptohost.com>",
  "contributors": [{
    "name": "Sebastian Rivera",
    "email": "sebastian@captis.app",
    "url": "https://sebas-rivera.com"
  }],
  "keywords": [
    "thumbnail",
    "video",
    "ffmpeg",
    "picture",
    "image",
    "resize",
    "webm",
    "mp4",
    "png",
    "preview",
    "url"
  ],
  "scripts": {
